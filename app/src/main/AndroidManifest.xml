<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dosta.dmaths">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_dmaths"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_dmaths_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".squareRoot"></activity>
        <activity android:name=".square"></activity>
        <activity android:name=".mathTable"></activity>
        <activity android:name=".division"></activity>
        <activity android:name=".multiplication"></activity>
        <activity android:name=".addition"></activity>
        <activity android:name=".subtraction"></activity>
        <activity android:name=".OperationType"></activity>
        <activity android:name=".levels"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>